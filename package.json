{
  "name": "react-movies",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "13.5.0",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-intersection-observer": "^9.1.0",
    "react-query": "3.35.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "swiper": "^8.1.4",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.0-alpha.64",
    "@storybook/addon-essentials": "^6.5.0-alpha.64",
    "@storybook/addon-interactions": "^6.5.0-alpha.64",
    "@storybook/addon-links": "^6.5.0-alpha.64",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.0-alpha.64",
    "@storybook/manager-webpack5": "^6.5.0-alpha.64",
    "@storybook/node-logger": "^6.5.0-alpha.64",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.0-alpha.64",
    "@storybook/testing-library": "^0.0.11",
    "autoprefixer": "10.4.4",
    "postcss": "8.4.12",
    "standard": "16.0.4",
    "storybook-dark-mode": "^1.0.9",
    "tailwindcss": "3.0.24",
    "webpack": "^5.72.0"
  },
  "standard": {
    "env": {
      "jest": true
    }
  }
